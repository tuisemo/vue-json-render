{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vue 3",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "target": "ES2022",
    "types": ["vite/client"]
  },
  "vueCompilerOptions": {
    "target": 3
  }
}
